		direccionTiro = direccionArquero ifTrue: [
			delantero fuerza > (arquero fuerza) ifTrue: [
				self sumarPuntajeDelantero.
			] ifFalse: [
				self sumarPuntajeArquero.
			].
		] ifFalse: [
			self sumarPuntajeDelantero.
		].
	].
