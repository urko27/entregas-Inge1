fuerzaCon: unaPelota
	unaPelota esPlayera ifTrue: [
		tiro = #Colocar ifTrue: [^(fuerzaBase/2)].
		tiro = #Matar ifTrue: [^fuerzaBase].
	].
	! !
!Delantero methodsFor: 'as yet unclassified' stamp: 'manuel urkovich 9/25/2023 18:37:26' prior: 50659624!
fuerzaCon: unaPelota
	unaPelota esPlayera ifTrue: [
		tiro = #Colocar ifTrue: [^(fuerzaBase/2) - (fuerzaBase * 0.2)].
		tiro = #Matar ifTrue: [^fuerzaBase - (fuerzaBase*0.2)].
	].
	unaPelota esJabulani ifTrue: [
		tiro = #Colocar ifTrue: [^(fuerzaBase/2)].
		tiro = #Matar ifTrue: [^fuerzaBase].
	]
	! !
